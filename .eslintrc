{
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6
  },
  "env": {
    "es6": true,
    "browser": true
  },
  "globals": {
    "React": true,
    "ReactDom": true,
    "PropTypes": true,
    "process": true,
    "__DEV__": true
  },
  "rules": {
    "no-console": 0,
    "react/prop-types": 0,
    "react/display-name": 0,
    "react/no-deprecated": 0
  }
}